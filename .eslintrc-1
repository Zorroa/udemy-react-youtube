{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "ecmaFeatures": {
        "jsx": true
    },
    "globals": {
        "window": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    }
}
